create database addressbook_service;

use addressbook_service;

CREATE TABLE contacts (
    id INT AUTO_INCREMENT PRIMARY KEY,

    first_name VARCHAR(50) NOT NULL,
    last_name  VARCHAR(50) NOT NULL,

    address VARCHAR(255),
    city    VARCHAR(50),
    state   VARCHAR(50),
    zip     VARCHAR(10),

    phone VARCHAR(15),
    email VARCHAR(100),

    date_added DATE
);